  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-modules-amd": "^7.7.5",
    "@babel/plugin-transform-modules-systemjs": "^7.7.4",
    "@babel/plugin-transform-modules-umd": "^7.7.4",
    "@babel/preset-env": "^7.7.7"
  }
}
